{
  "name": "TournamentMango",
  "description": "A tournament management tool.",
  "readme": "README.md",
  "repository": {
    "url": "https://github.com/seiyria/tournamentmango"
  },
  "version": "0.6.8",
  "license": "MIT",
  "authors": [
    "Kyle Kemp <kyle@seiyria.com>"
  ],
  "node-main": "nw.js",
  "main": "nw.html",
  "window": {
    "title": "TournamentMango - loading...",
    "width": 1024,
    "height": 768,
    "toolbar": false
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babelify": "^6.2.0",
    "bower": "~1.6.3",
    "browser-sync": "^2.9.11",
    "browserify": "~11.2.0",
    "conventional-changelog": "^0.5.0",
    "del": "~2.0.2",
    "errorify": "^0.3.1",
    "eslint-config-kellyirc": "^3.1.0",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "^3.0.2",
    "gulp-bump": "^1.0.0",
    "gulp-concat": "~2.6.0",
    "gulp-eslint": "^1.0.0",
    "gulp-filter": "~3.0.1",
    "gulp-folders": "^1.1.0",
    "gulp-gh-pages": "~0.5.2",
    "gulp-git": "^1.5.0",
    "gulp-github-release": "^1.0.3",
    "gulp-if": "^2.0.0",
    "gulp-jade": "~1.1.0",
    "gulp-minify-css": "~1.2.1",
    "gulp-ng-annotate": "^1.1.0",
    "gulp-nw-builder": "^1.3.1",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-tag-version": "~1.3.0",
    "gulp-util": "~3.0.6",
    "gulp-zip": "^3.0.2",
    "lodash": "^3.10.1",
    "run-sequence": "^1.1.4",
    "vinyl-paths": "^2.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.4.0"
  },
  "scripts": {
    "postinstall": "node ./node_modules/bower/bin/bower install",
    "start": "node ./node_modules/gulp/bin/gulp.js",
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "test": "node ./node_modules/gulp/bin/gulp.js check"
  },
  "gulp": {
    "sass": "src/scss/*.scss",
    "libjs": [
      "./bower_components/tournament/dist/*.js",
      "./bower_components/firebase/firebase.js",
      "./bower_components/lodash/lodash.min.js",
      "./bower_components/svg-morpheus/compile/minified/svg-morpheus.js",
      "./bower_components/FileSaver.js/FileSaver.min.js",
      "./bower_components/clipboard/dist/clipboard.min.js",
      "./node_modules/angular/angular.min.js",
      "./bower_components/jquery/dist/jquery.min.js",
      "./node_modules/angular-aria/angular-aria.min.js",
      "./node_modules/angular-animate/angular-animate.min.js",
      "./node_modules/angular-material/angular-material.min.js",
      "./node_modules/angular-ui-router/build/angular-ui-router.min.js",
      "./node_modules/angular-material-icons/angular-material-icons.min.js",
      "./node_modules/angular-material-data-table/dist/md-data-table.min.js",
      "./node_modules/ngstorage/ngStorage.min.js",
      "./node_modules/angularfire/dist/angularfire.min.js",
      "./bower_components/angular-sortable-view/src/angular-sortable-view.min.js",
      "./node_modules/angular-sanitize/angular-sanitize.min.js",
      "./bower_components/ng-inline-edit/dist/ng-inline-edit.min.js"
    ],
    "libcss": [
      "./node_modules/angular-material/angular-material.css",
      "./node_modules/angular-material-data-table/dist/md-data-table.min.css",
      "./bower_components/ng-inline-edit/dist/ng-inline-edit.min.css"
    ],
    "root": [
      "CNAME",
      "favicon.ico"
    ],
    "assets": [
      "src/assets/**/*"
    ],
    "jade": [
      "src/jade/**/*.jade"
    ],
    "js": [
      "src/js/**/*.js"
    ],
    "testjs": [
      "test/**/*.js"
    ],
    "bin": {
      "build": "./bin-build",
      "release": "./bin-release",
      "cache": "./bin-cache"
    },
    "entry": "./src/js/_init.js",
    "dist": "./dist/"
  },
  "dependencies": {
    "angular": "^1.4.6",
    "angular-animate": "^1.4.6",
    "angular-aria": "^1.4.6",
    "angular-material": "^0.11.4",
    "angular-material-data-table": "^0.8.11",
    "angular-material-icons": "^0.6.0",
    "angular-sanitize": "^1.4.7",
    "angular-ui-router": "^0.2.15",
    "angularfire": "^1.1.2",
    "ngstorage": "^0.3.9"
  }
}
